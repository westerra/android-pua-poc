<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:top="?spacerSmall"
        android:bottom="?spacerSmall">
        <selector>
            <item android:state_selected="true">
                <shape>
                    <solid android:color="@color/tabBackgroundColor"/>
                    <stroke android:color="@color/tabUnselectedBackgroundColor"
                        android:width="2dp" />
                </shape>
            </item>

            <item android:state_selected="false">
                <shape>
                    <solid android:color="@color/tabUnselectedBackgroundColor"/>
                    <stroke android:color="@color/tabUnselectedBackgroundColor"
                        android:width="0dp" />
                </shape>
            </item>

        </selector>
    </item>
</layer-list>